  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.0",
    "mocha": "^3.4.2",
    "rewire": "^2.5.2"
  },
  "dependencies": {
    "node-fetch": "^1.7.1"
  }
}
