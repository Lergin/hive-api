{
  "name": "hive-api",
  "version": "1.0.11",
  "description": "a wrapper for the hivemc api",
  "main": "./build/lib/main.js",
  "types": "./build/lib/main.d.ts",
  "scripts": {
    "start": "node index.js",
    "test": "tsc && mocha **/*.test.js",
